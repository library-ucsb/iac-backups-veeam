{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
               "s3:ListAllMyBuckets",
               "s3:GetBucketLocation"
            ],
            "Resource": "arn:aws:s3:::*" 
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": "arn:aws:s3:::ucsb-library-veeam-s3-storage" 
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject", 
                "s3:DeleteObject", 
                "s3:AbortMultipartUpload" 
            ],
            "Resource": "arn:aws:s3:::ucsb-library-veeam-s3-storage/*" 
        }
    ]
}